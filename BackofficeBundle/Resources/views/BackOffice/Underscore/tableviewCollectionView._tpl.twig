{% include 'OpenOrchestraBackofficeBundle:BackOffice:Underscore/Jarvis/header._tpl.twig' %}

<div role="container">
    <table id="tableviewCollectionTable">
        <thead>
            <tr>
                <% _.each(displayedElements, function(displayedElement) { %>
                    <td><%= displayedElement %></td>
                <% }); %>
                <td></td>
            </tr>
        </thead>
        <tbody>
        </tbody>
    </table>
    <% if (typeof links !== 'undefined' && typeof links._self_add !== 'undefined' ) { %>
        <div>
            <a href="<%= links._self_add %>" data-url="<%= links._self_add %>" class="btn btn-success ajax-add bh-button-add">{{ 'open_orchestra_backoffice.form.add'|trans({}, 'messages', language) }}</a>
        </div>
    <% } %>
</div>

{% include 'OpenOrchestraBackofficeBundle:BackOffice:Underscore/Jarvis/footer._tpl.twig' %}
